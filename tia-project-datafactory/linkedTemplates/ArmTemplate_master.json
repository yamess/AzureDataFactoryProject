{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tia-project-datafactory"
		},
		"AdventureWorks2014Link_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdventureWorks2014Link'"
		},
		"SalesOrderHeaderLink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SalesOrderHeaderLink'"
		},
		"TiaBlobStorageLink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TiaBlobStorageLink'"
		},
		"outdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'outdb'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorks2014Link_connectionString": {
						"value": "[parameters('AdventureWorks2014Link_connectionString')]"
					},
					"SalesOrderHeaderLink_connectionString": {
						"value": "[parameters('SalesOrderHeaderLink_connectionString')]"
					},
					"TiaBlobStorageLink_connectionString": {
						"value": "[parameters('TiaBlobStorageLink_connectionString')]"
					},
					"outdb_connectionString": {
						"value": "[parameters('outdb_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}